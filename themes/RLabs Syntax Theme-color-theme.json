      ],
      "settings": {
        "foreground": "#c26b6b"
      }
    },
    {
      "name": "HTML tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#c26b6b"
      }
    },
    {
      "name": "CSS attribute parent selectors ('&')",
      "scope": ["entity.other.attribute-name.parent-selector"],
      "settings": {
        "foreground": "#c26b6b"
      }
    },
    {
      "name": "HTML/CSS attribute names",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#aabb96",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML Tag Text",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#FFFFFF"
        //"fontStyle": "bold"
      }
    },
    {
      "name": "Operator, Misc",
